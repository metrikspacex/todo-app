{
  "name": "@metrikspacex/todo-app-frontend",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://metrikspacex.github.io/todo-app/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metrikspacex/todo-app.git",
    "directory": "packages/frontend"
  },
  "license": "MIT",
  "author": "Matthew Mason",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "eslint": "eslint --cache --max-warnings 0 --ext cjs,js,jsx,ts,tsx .",
    "prettier": "prettier --check .",
    "preview": "vite preview",
    "test": "pnpm test:typecheck && pnpm eslint && pnpm test:unit && pnpm test:cypress && pnpm test:package",
    "test:cypress": "pnpm test:cypress:component && pnpm test:cypress:e2e",
    "test:cypress:component": "pnpm cypress run --component",
    "test:cypress:e2e": "pnpm cypress run --e2e",
    "test:package": "publint --strict",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:ui": "vitest --ui",
    "test:unit": "vitest --run --reporter=verbose",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "@tanstack/react-query": "^5.4.3",
    "@tanstack/react-query-devtools": "^5.4.3",
