{
  "name": "@metrikspacex/todo-app-tools",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://metrikspacex.github.io/todo-app/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metrikspacex/todo-app.git",
    "directory": "packages/tools"
  },
  "license": "MIT",
  "author": "Matthew Mason",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": null,
    "./gre": {
      "import": {
        "types": "./dist/gre.d.ts",
        "default": "./dist/gre.mjs"
      },
      "require": {
        "types": "./dist/gre.d.cts",
        "default": "./dist/gre.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "gre": "./dist/gre.cjs"
  },
  "files": [
    "bin",
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "pnpm unbuild",
    "eslint": "eslint --cache --max-warnings 0 .",
    "gre": "jiti ./src/gre",
    "prepack": "rm -rf dist && pnpm build",
    "prettier": "prettier --check .",
    "stub": "pnpm unbuild --stub",
    "test": "pnpm test:typecheck && pnpm eslint && pnpm test:unit && pnpm test:package",
    "test:package": "publint",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:ui": "vitest --ui",
    "test:unit": "vitest --run --reporter=verbose",
    "test:watch": "vitest"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "chokidar": "3.5.3",
    "commander": "^11.1.0",
    "esbuild": "^0.19.5",
    "eslint-define-config": "^1.24.1",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "glob": "10.3.10",
    "inquirer": "9.2.11",
    "mkdirp": "^3.0.1",
    "rimraf": "^5.0.5",
    "shelljs": "0.8.5",
    "source-map-support": "0.5.21",
    "tslib": "^2.6.2",
    "unbuild": "^2.0.0"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vitest/ui": "^0.34.7",
    "eslint": "^8.52.0",
    "eslint-plugin-todo-app": "0.0.0",
    "jiti": "^1.20.0",
    "prettier": "^3.0.3",
    "publint": "^0.2.5",
    "tsup": "^7.2.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "0.34.6"
  }
}
